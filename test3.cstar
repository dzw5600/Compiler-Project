// test.cstar example

/* Multi-line
   comment test */

int count = 0;

while(count <= 20)
{
   if(count % 3 = 0 && count % 5 = 0)
   {
      print("FizzBuzz");
   }
   elif(count % 3 = 0)
   {
      print("Fizz");
   }
   else if(count % 5 = 0)
   {
      print("Buzz");
   }
   else
   {
      print(count);
   }
   set count = count + 1;
}