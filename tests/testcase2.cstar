// Test fizzbuzz program:
// 1. While loops
// 2. If/elif/else code blocks
// 3. Operators like modulus, equality comparison (single `=` for equality), boolean comparison

int count = 0;

while(count <= 20)
{
   if(count % 3 = 0 && count % 5 = 0)
   {
      print("FizzBuzz");
   }
   else if(count % 3 = 0)
   {
      print("Fizz");
   }
   else if(count % 5 = 0)
   {
      print("Buzz");
   }
   else
   {
      print(count);
   }
   set count = count + 1;
}
